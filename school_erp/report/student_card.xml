<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report_student">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
               <h2>
                   Student Report
               </h2>
                <t t-esc="doc.name"/>
                <t t-esc="doc.student_age"/>
                <t t-esc="doc.gender_male"/>
            </t>
        </t>
    </template>
</odoo>